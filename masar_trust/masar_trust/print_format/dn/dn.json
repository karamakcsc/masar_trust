 "html": "{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\r\n    {% if letter_head and not no_letterhead %}\r\n    <div class=\"letter-head\">{{ letter_head }}</div>\r\n    {% endif %}\r\n    {% if letter_footer and not no_letterfooter %}\r\n    <div class=\"letter-head\">{{ letter_footer }}</div>\r\n    {% endif %}\r\n    {%- if doc.meta.is_submittable and doc.docstatus==0-%}\r\n    <div class=\"alert alert-info text-center\">\r\n        <h4 style=\"margin: 0px;\">{{ _(\"DRAFT\") }}</h4></div>\r\n    {%- endif -%}\r\n    {%- if doc.meta.is_submittable and doc.docstatus==2-%}\r\n    <div class=\"alert alert-danger text-center\">\r\n        <h4 style=\"margin: 0px;\">{{ _(\"CANCELLED\") }}</h4></div>\r\n    {%- endif -%}\r\n    {% if max_pages > 1 %}\r\n    <p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\r\n    {% endif %}\r\n{%- endmacro -%}\r\n{{ add_header(0,1,doc,letter_head, no_letterhead) }}\r\n<label>\r\n<p class=\"text-center\" style=\"font-size:30px; background-color:Gainsboro;\"><b>{{ _(\"Delivery Note\") }}</b></p><br>\r\n<div class=\"print-heading text-left\">\t\t\t\t\r\n    <b><h6>\r\n        {%  if frappe.get_list(\"Access Log\", filters={\r\n                        \"reference_document\": doc.name, \r\n                        \"file_type\": \"PDF\",\r\n                        \"method\": \"Print\"\r\n                    })|length > 1 \r\n        %}\r\n            <div>\u0646\u0633\u062e\u0629 \u0627\u0627\u0644\u0645\u062d\u0627\u0633\u0628\u0629 \u0637\u0628\u0642 \u0627\u0644\u0627\u0635\u0644</div>\r\n        {% else %}\r\n            <div>\u0646\u0633\u062e\u0629 \u0627\u0644\u0645\u062d\u0627\u0633\u0628\u0629 \u0627\u0644\u0623\u0635\u0644\u064a\u0629</div>\r\n        {% endif %}\r\n    </b></h6>\r\n</div>\r\n</label>\r\n<div class=\"row\">\r\n            <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Date..\") }}:</label></div>\r\n            <div class=\"col-xs-4 text-left\" style=\"text-align: right; text-left: 0px;\">{{doc.get_formatted(\"posting_date\") or ''}}</div>\r\n            <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Delivery Note No.\") }}:</label></div>\r\n            <div class=\"col-xs-4 text-left\" style=\"font-weight: bold; text-align: right; text-left: 0px;\">{{doc.name or ''}}</div>          \r\n        </div>\r\n<div class=\"row\">\r\n            <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Customer Name\") }}:</label></div>\r\n            <div class=\"col-xs-4 text-left\" style=\"font-weight: bold; text-align: right; text-left: 0px;\">{{doc.customer or ''}}</div>\r\n            <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Set Source Warehouse\") }}:</label></div>\r\n            <div class=\"col-xs-4 text-left\" style=\"text-align: right; text-left: 0px;\">{{doc.set_warehouse or ''}}</div>\r\n        </div>\r\n<div class=\"row\">\r\n            <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Client Account\") }}:</label></div>\r\n            <div class=\"col-xs-4 text-left\" style=\"text-align: right; text-left: 0px;\">{{doc.customer_sub or ''}}</div>\r\n            <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Sales Order No..\") }}:</label></div>\r\n            {%- for row in doc.items|unique(attribute=\"against_sales_order\") -%}<div class=\"col-xs-4 text-left\" style=\"text-align: right; text-left: 0px;\">{{row.against_sales_order or ''}}</div> {%- endfor -%}\r\n        </div> \r\n<div class=\"row\">\r\n            <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Branch\") }}:</label></div>\r\n            <div class=\"col-xs-4 text-left\" style=\"text-align: right; text-left: 0px;\">{{doc.branch or ''}}</div>\r\n            <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Sales Person.\") }}:</label></div>\r\n            {%- for row in doc.sales_team -%}\r\n            <div class=\"col-xs-4 text-left\" style=\"text-align: right; text-left: 0px;\">{{row.sales_person or ''}}</div>{%- endfor -%}\r\n        </div>     \r\n<br>\r\n<table class=\"table table-bordered\">\r\n\t<tbody>\r\n\t\t<tr>\r\n\t\t\t<th>{{ _(\"Sr\") }}</th>\r\n\t\t\t<th>{{ _(\"Description\") }}</th>\r\n\t\t\t<th class=\"text-center\">{{ _(\"Item Code\") }}</th>\r\n\t\t\t<th class=\"text-center\">{{ _(\"Quantity\") }}</th>\r\n\t\t</tr>\r\n\t\t{%- for row in doc.items -%}\r\n\t\t<tr>\r\n\t\t\t<td style=\"width: 3%;\">{{ row.idx }}</td>\r\n\t\t\t<td style=\"width: 47%;\">\r\n\t\t\t\t<div style=\"border: 0px;\">{{ row.description or '' }}</div></td>\r\n\t\t\t<td style=\"width: 30%; text-align: center;\">\r\n\t\t\t\t<div style=\"border: 0px;\">{{ row.item_code or '' }}</div></td>\t\r\n\t\t\t<td style=\"width: 20%; text-align: center;\">{{ row.qty }}</td>\r\n\t\t</tr>\r\n\t\t{%- endfor -%}\r\n\t\t<tr>\r\n\t\t\t<th></th>\r\n\t\t\t<th></th>\r\n\t\t\t<th class=\"text-center\">{{ _(\"Total Quantity\") }}</th>\r\n\t\t\t<th class=\"text-center\">{{doc.get_formatted(\"total_qty\") or '' }}</th>\r\n\t\t</tr>\r\n\t</tbody>\r\n</table>\r\n<div class=\"row\">\r\n        <div class=\"col-xs-2 text-left\" style=\"text-align:right; text-left: 0px;\"><label>{{ _(\"Driver Name\") }}:</label></div>\r\n        <div class=\"col-xs-4 text-left\" style=\"text-align: right; text-left: 0px;\">{{doc.driver_name or ''}}</div>\r\n        <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Transportation Fees\") }}:</label></div>\r\n        <div class=\"col-xs-4 text-left\" style=\"font-weight: bold; text-align: right; text-left: 0px;\">{{doc.get_formatted(\"transportation_fees\") or ''}}</div>\r\n       </div> \r\n<div class=\"row\">   <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Vehicle No\") }}:</label></div>\r\n        <div class=\"col-xs-4 text-left\" style=\"font-weight: bold; text-align: right; text-left: 0px;\">{{doc.vehicle_no or ''}}</div>\r\n        <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-left: 0px;\"><label></label></div>\r\n        <div class=\"col-xs-4 text-left\" style=\"text-align: right; text-left: 0px;\"></div>\r\n        </div>\r\n<div class=\"row\">\r\n        <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Karwa\") }}:</label></div>\r\n        <div class=\"col-xs-4 text-left\" style=\"font-weight: bold; text-align: right; text-left: 0px;\">{{doc.get_formatted(\"karwa\") or ''}}</div>\r\n        <div class=\"col-xs-2 text-left\" style=\"text-align: right; text-left: 0px;\"><label></label></div>\r\n        <div class=\"col-xs-4 text-left\" style=\"text-align: right; text-left: 0px;\"></div>\r\n        </div>\r\n<br>\r\n<table class=\"table table-bordered\">\r\n        <tr>\r\n            <th class=\"text-center\">{{ _(\"Comments\") }}</th>\r\n        </tr> \r\n        <tr>\r\n        <td style=\"width: 100%; text-align: left;\">{{doc.notes or '' }}</td>\r\n        </tr>\r\n     </tbody>\r\n</table>\r\n<br>\r\n<table class=\"table table-bordered\">\r\n      <tbody>\r\n        <tr>\r\n            <th class=\"text-center\">{{ _(\"Co-ordinator\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Warehouse Manager\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Packing Person\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Driver.\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Customer.\") }}</th>\r\n         </tr>   \r\n        <tr>\r\n            <td style=\"width: 20%;line-height: 3.0; text-align: center;\"><b>...............</b></td>\r\n            <td style=\"width: 20%;line-height: 3.0; text-align: center;\"><b>...............</b></td>\r\n            <td style=\"width: 20%;line-height: 3.0; text-align: center;\"><b>...............</b></td>\r\n            <td style=\"width: 20%;line-height: 3.0; text-align: center;\"><b>...............</b></td>\r\n            <td style=\"width: 20%;line-height: 3.0; text-align: center;\"><b>...............</b></td>\r\n        </tr>\r\n    </tbody>\r\n</table>\r\n<div class=\"page-break\"></div>\r\n{%- macro add_footer(page_num, max_pages, doc, letter_head, no_letterhead) -%}\r\n{% if letter_head and not no_letterhead %}\r\n       <div class=\"footer-html\" style=\"position: fixed; left: 0; bottom: 1;width: 100%; text-align: center;\">{{ footer }}</div>\r\n       {% endif %}\r\n       \r\n {%- endmacro -%}\r\n\r\n{{ add_footer (0,1,doc,letter_head, no_letterhead) }}",

 "html": "{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\r\n    {% if letter_head and not no_letterhead %}\r\n    <div class=\"letter-head\">{{ letter_head }}</div>\r\n    {% endif %}\r\n    {% if letter_footer and not no_letterfooter %}\r\n    <div class=\"letter-head\">{{ letter_footer }}</div>\r\n    {% endif %}\r\n    {%- if doc.meta.is_submittable and doc.docstatus==0-%}\r\n    <div class=\"alert alert-info text-center\">\r\n        <h4 style=\"margin: 0px;\">{{ _(\"DRAFT\") }}</h4></div>\r\n    {%- endif -%}\r\n    {%- if doc.meta.is_submittable and doc.docstatus==2-%}\r\n    <div class=\"alert alert-danger text-center\">\r\n        <h4 style=\"margin: 0px;\">{{ _(\"CANCELLED\") }}</h4></div>\r\n    {%- endif -%}\r\n    {% if max_pages > 1 %}\r\n    <p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\r\n    {% endif %}\r\n{%- endmacro -%}\r\n{{ add_header(0,1,doc,letter_head, no_letterhead) }}\r\n<label>\r\n<p class=\"text-center\" style=\"font-size:30px; background-color:Gainsboro;\"><b>{{ _(doc.purpose) }}</b></p><br>\r\n</label>\r\n<div class=\"row\">\r\n            <div class=\"col-xs-3 text-left\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Stock Entry No.\") }}:</label></div>\r\n            <div class=\"col-xs-4 text-left\" style=\"font-weight: bold; text-align: right; text-left: 0px;\">{{doc.name or ''}}</div>\r\n            </div>\r\n<div class=\"row\">\r\n            <div class=\"col-xs-3 text-left\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Date....\") }}:</label></div>\r\n            <div class=\"col-xs-4 text-left\" style=\"text-align: right; text-left: 0px;\">{{doc.get_formatted(\"posting_date\") or ''}}</div>\r\n        </div>\r\n <div class=\"row\">\r\n            <div class=\"col-xs-3 text-left\" style=\"text-align: right; text-left: 0px;\"><label>{{ _(\"Default Target Warehouse.\") }}:</label></div>\r\n            <div class=\"col-xs-4 text-left\" style=\"text-align: right; text-left: 0px;\">{{doc.from_warehouse or ''}}</div>\r\n            </div>\r\n<table class=\"table table-bordered\">\r\n\t<tbody>\r\n\t\t<tr>\r\n\t\t\t<th>{{ _(\"Sr\") }}</th>\r\n\t\t\t<th class=\"text-center\">{{ _(\"Description\") }}</th>\r\n\t\t\t<th class=\"text-center\">{{ _(\"Item Code\") }}</th>\r\n\t\t\t<th class=\"text-center\">{{ _(\"Qty\") }}</th>\r\n\t\t</tr>\r\n\t\t{%- for row in doc.items -%}\r\n\t\t<tr>\r\n\t\t\t<td style=\"width: 3%;\">{{ row.idx }}</td>\r\n\t\t\t<td style=\"width: 45%;\"><div style=\"border: 0px;\">{{ row.description or '' }}</div></td>\r\n\t\t\t<td style=\"width: 30%; text-align: center;\"><div style=\"border: 0px;\">{{ row.item_code or '' }}</div></td>\t\r\n\t\t\t<td style=\"width: 10%; text-align: center;\">{{ row.qty }}</td>\r\n\t\t</tr>\r\n\t\t{%- endfor -%}\r\n\t</tbody>\r\n</table>\r\n\r\n<br>\r\n<table class=\"table table-bordered\">\r\n        <tr>\r\n            <th class=\"text-center\">{{ _(\"Comments\") }}</th>\r\n        </tr> \r\n        <tr>\r\n        <td style=\"width: 100%; text-align: left;\">{{ doc.remarks or '' }}</td>\r\n        </tr>\r\n     </tbody>\r\n</table> \r\n<br>\r\n<table class=\"table table-bordered\">\r\n        <tr>\r\n            <th class=\"text-center\">{{ _(\"Co-ordinator\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Warehouse Manager\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Dispatch Officer\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Driver...\") }}</th>\r\n         </tr>   \r\n        <tr>\r\n            <td style=\"width: 25%;line-height: 3.0; text-align: center;\"><b>...............</b></td>\r\n            <td style=\"width: 25%;line-height: 3.0; text-align: center;\"><b>...............</b></td>\r\n            <td style=\"width: 25%;line-height: 3.0; text-align: center;\"><b>...............</b></td>\r\n            <td style=\"width: 25%;line-height: 3.0; text-align: center;\"><b>...............</b></td>\r\n        </tr>\r\n    </tbody>\r\n</table>\r\n<!--<div class=\"page-break\"></div>-->\r\n{%- macro add_footer(page_num, max_pages, doc, letter_head, no_letterhead) -%}\r\n{% if letter_head and not no_letterhead %}\r\n       <div class=\"footer-html\" style=\"position: fixed; left: 0; bottom: 1;width: 100%; text-align: center;\">{{ footer }}</div>\r\n       {% endif %}\r\n       \r\n {%- endmacro -%}\r\n\r\n{{ add_footer (0,1,doc,letter_head, no_letterhead) }}",

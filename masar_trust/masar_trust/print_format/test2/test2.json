 "html": "{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\r\n    {% if letter_head and not no_letterhead %}\r\n    <div class=\"letter-head\">{{ letter_head }}</div>\r\n    {% endif %}\r\n    {% if letter_footer and not no_letterfooter %}\r\n    <div class=\"letter-head\">{{ letter_footer }}</div>\r\n    {% endif %}\r\n    {%- if doc.meta.is_submittable and doc.docstatus==0-%}\r\n    <div class=\"alert alert-info text-center\">\r\n        <h4 style=\"margin: 0px;\">{{ _(\"DRAFT\") }}</h4></div>\r\n    {%- endif -%}\r\n    {%- if doc.meta.is_submittable and doc.docstatus==2-%}\r\n    <div class=\"alert alert-danger text-center\">\r\n        <h4 style=\"margin: 0px;\">{{ _(\"CANCELLED\") }}</h4></div>\r\n    {%- endif -%}\r\n    {% if max_pages > 1 %}\r\n    <p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\r\n    {% endif %}\r\n{%- endmacro -%}\r\n\r\n{{ add_header(0,1,doc,letter_head, no_letterhead) }}\r\n<label>\r\n<p class=\"text-center\" style=\"font-size:20px; background-color:Gainsboro;\"><b>{{ _(\"Sales Order\") }}</b></p><br>\r\n<!--<p class=\"text-center\"><b>{{doc.name or ''}} / {{ _(doc.mode_of_payment) }}</b></p><br>-->\r\n</label>\r\n<div class=\"row\">        \r\n     <div class=\"col-xs-6\">            \r\n        <div class=\"row\">\r\n            <div class=\"col-xs-2 text-center\" style=\" left:-20px;\"><label>{{ _(\"Date\") }}:</label></div>\r\n            <div class=\"col-xs-4\" style=\" center:-20px;\">{{ doc.get_formatted(\"transaction_date\") or ''}}</div>\r\n        </div>\r\n        <div class=\"row\">\r\n            <div class=\"col-xs-2 text-center\" style=\"left:-20px;\"><label>{{ _(\"Currency\") }}:</label></div>\r\n            <div class=\"col-xs-4 \" style=\"center:-20px;\">{{ doc.get_formatted(\"currency\") or ''}}</div>\r\n        </div>         \r\n    </div>\r\n<div class=\"row\">        \r\n     <div class=\"col-xs-6\">            \r\n        <div class=\"row\">\r\n            <div class=\"col-xs-5 text-right\" style=\"position:absolute; right:-200px;\"><label>{{ _(\"Delivery Date\") }}:</label></div>\r\n            <div class=\"col-xs-7 \" style=\"position:absolute; right:-48px;\">{{ doc.get_formatted(\"delivery_date\") or ''}}</div>\r\n            <div class=\"col-xs-5 text-right\"><label>{{ _(\"Sales Order No.\") }}:</label></div>\r\n            <div class=\"col-xs-7 \">{{doc.name or ''}}</div>\r\n        </div>\r\n        <div class=\"row\">\r\n            <div class=\"col-xs-5 text-right\" style=\"position:absolute; right:-275px;\"><label>{{ _(\"Branch\") }}:</label></div>\r\n            <div class=\"col-xs-7 \" style=\"position:absolute; right:-135px;\">{{doc.branch or ''}} </div>\r\n            <div class=\"col-xs-5 text-right\"><label>{{ _(\"Cost Center\") }}:</label></div>\r\n            <div class=\"col-xs-7 \">{{doc.cost_center or ''}} </div>\r\n        </div>        \r\n    </div>\r\n<div class=\"row\">        \r\n     <div class=\"col-xs-6\">            \r\n        <div class=\"row\">\r\n            <div class=\"col-xs-5 text-center\" style=\"position:absolute; right:8px;\"><label>{{ _(\"Customer Name\") }}:</label></div>\r\n            <div class=\"col-xs-7 \" style=\"position:absolute; right:105px;\">{{doc.customer or ''}}</div>\r\n        </div>\r\n    </div>\r\n    <div class=\"col-xs-6\">\r\n        <div class=\"row\">\r\n            <div class=\"col-xs-5 text-right\" style=\"right:-6px;\"> <label>{{ _(\"Mode Of Payment\") }}:</label> </div>\r\n            <div class=\"col-xs-7 \">{{ doc.mode_of_payment or ''}}</div>\r\n        </div>\r\n       <!-- <div class=\"row\">\r\n            <div class=\"col-xs-5 text-right\"> <label>{{ _(\"Mobaile No\") }}:</label> </div>\r\n            <div class=\"col-xs-7 \">{{ doc.contact_mobile or ''}}</div>\r\n        </div>-->\r\n</div>\r\n<div class=\"col-xs-6\">\r\n        <div class=\"row\">\r\n            <div class=\"col-xs-5 text-right\" style=\"right:-15px;\"><label>{{ _(\"Client Account\") }}:</label></div>\r\n            <div class=\"col-xs-7 \" style=\" right:-30px;\">{{doc.customer_sub or ''}}</div>\r\n        </div>\r\n       <!-- <div class=\"row\">\r\n            <div class=\"col-xs-5 text-right\"> <label>{{ _(\"Mobaile No\") }}:</label> </div>\r\n            <div class=\"col-xs-7 \">{{ doc.contact_mobile or ''}}</div>\r\n        </div>-->\r\n</div>\r\n<br>\r\n<br>\r\n<br>\r\n<br>\r\n<table class=\"table table-condensed table-hover table-bordered\">\r\n        <tr>\r\n            <th class=\"text-right\">{{ _(\"Sr\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Description\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Item Code\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Quantity\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Price\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Net Total Befor Dis\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Discount\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Net Total\") }}</th>\r\n        </tr>\r\n        {%- for row in doc.items -%}\r\n        <tr>\r\n            <td style=\"width: 1%;text-align: left;\">{{ row.idx }}</td>\r\n            <td style=\"width: 4%;\">\r\n                 <b>{{ row.description or '' }} </b> </td>\r\n               <td style=\"width: 1%;text-align: center;\"> {{ row.item_code or '' }}  </td>\r\n            <td style=\"width: 1%;text-align: center;\">{{ row.qty }}</td>\r\n            <td style=\"width: 1%;text-align: center;\">\r\n                {{ row.get_formatted(\"rate\", doc) or ''}}\r\n            <td style=\"width: 1%;text-align: center;\">{{\r\n                row.get_formatted(\"base_price_list_rate\", doc) or ''}}</td>\r\n            <td style=\"width: 1%;text-align: center;\">{{\r\n                row.get_formatted(\"discount_amount\", doc) or ''}}</td>\r\n                <td style=\"width: 1%;text-align: center;\">{{\r\n                row.get_formatted(\"amount\", doc) or ''}}</td>\r\n        </tr>\r\n        {%- endfor -%}\r\n    </tbody>\r\n</table>\r\n<br>\r\n   \r\n<div class=\"row\">\r\n    <div class=\"col-xs-6 text-right\"></div>\r\n    <div class=\"col-xs-4 text-right\"><label>{{ _(\"Net Total\") }}</label></div>\r\n    <div class=\"col-xs-2 \">\r\n      {{ doc.get_formatted(\"total\") or '' }}\r\n    </div>\r\n</div>  \r\n<!--<div class=\"row\">\r\n    <div class=\"col-xs-6 text-right\"></div>\r\n    <div class=\"col-xs-4 text-right\"><label>{{ _(\"Additional Discount\") }}</label></div>\r\n    <div class=\"col-xs-2 \">\r\n      {{ doc.get_formatted(\"base_discount_amount\") or '' }}\r\n    </div>\r\n</div> --> \r\n{%- for row in doc.taxes -%}\r\n    {%- if not row.included_in_print_rate -%}\r\n    {%- if row.tax_amount -%}      \r\n<div class=\"row\">\r\n    <div class=\"col-xs-6 text-right\"></div>\r\n    <div class=\"col-xs-3 text-right\"><label>{{ row.description }}</label></div>\r\n    <div class=\"col-xs-3 text-right\">{{ row.get_formatted(\"tax_amount\", doc) }}</div>\r\n</div> \r\n    {%- endif -%}\r\n    {%- endif -%}\r\n{%- endfor -%}\r\n\r\n{%- if doc.discount_amount -%}\r\n    <div class=\"row\">\r\n         <div class=\"col-xs-6 text-right\"></div>\r\n         <div class=\"col-xs-4 text-right\"><label>{{ _(\"Additional Discount\") }}</label></div>\r\n         <div class=\"col-xs-2 \">{{ doc.get_formatted(\"discount_amount\") }}</div>\r\n    </div> \r\n {%- endif -%}         \r\n           \r\n<div class=\"row\">\r\n    <div class=\"col-xs-6 text-right\"></div>\r\n    <div class=\"col-xs-4 text-right\"><label>{{ _(\"Required Total\") }}</label></div>\r\n    <div class=\"col-xs-2 \">{{ doc.get_formatted(\"grand_total\") }}</div>\r\n</div>\r\n<br>\r\n<!--<div class=\"row\">\r\n <div class=\"col-xs-2 text-right\"><b>{{ _(\"Approval Managment\") }}</b></div>\r\n</div>-->\r\n\r\n<table class=\"table table-bordered\">\r\n        <tr>\r\n            <th class=\"text-center\">{{ _(\"Co-ordinator\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Sales Manager\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Executive Director\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Agent\") }}</th>\r\n         </tr>   \r\n        <tr>\r\n            <td style=\"width: 25%;line-height: 3.0; text-align: Center;\"><b>...............</b></td>\r\n            <td style=\"width: 25%;line-height: 3.0; text-align: center;\"><b>...............</b> </td>\r\n            <td style=\"width: 25%;line-height: 3.0; text-align: center;\"><b>...............</b> </td>\r\n            <td style=\"width: 25%;line-height: 3.0; text-align: center;\"><b>...............</b></td>\r\n        </tr>\r\n    </tbody>\r\n</table>\r\n<!--\r\n<div class=\"row\">\r\n <div class=\"col-xs-2 text-center\"><b>{{ _(\"Payment Terms\") or '' }}</b></div> \r\n</div>\r\n<table class=\"table table-condensed table-hover table-bordered\">\r\n        <tr>\r\n            <th class=\"text-right\" style=\"width: 1%;\">{{ _(\"Sr\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Payment Method\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Payment Date\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"Percentage\") }}</th>\r\n            <th class=\"text-center\">{{ _(\"The Amount Required\") }}</th>\r\n        </tr>\r\n  {%- for row in doc.payment_schedule -%}      \r\n        <tr>\r\n            <td style=\"width: 1%;text-align: left;\">{{ row.idx }}</td>\r\n            <td style=\"width: 10%;\">\r\n                 <b>{{ row.payment_term or '' }} </b> </td>\r\n               <td style=\"width: 5%; text-align: center;\"> {{ row.due_date or '' }}  </td>\r\n            <td style=\"width: 3%; text-align: center;\">{{ row.get_formatted(\"invoice_portion\", doc) or ''}}</td>\r\n            <td style=\"width: 3%; text-align: center;\">\r\n                {{ row.get_formatted(\"payment_amount\", doc) or ''}}</td>\r\n        </tr>\r\n        {%- endfor -%}\r\n    </tbody>\r\n</table>\r\n{%- if doc.terms -%}\r\n<div class=\"row\">\r\n <div class=\"col-xs-2 text-center\"><b>{{ _(\"Terms and Conditions\") }}</b></div>\r\n</div>\r\n<div class=\"row\">\r\n    <div class=\"col-xs-1 text-center\"></div>\r\n    <div class=\"col-xs-30 \">{{doc.terms or ''  }}</div>\r\n</div>\r\n\r\n{%- endif -%} \r\n<div class=\"page-break\"></div>\r\n<div id=\"footer-html\">\r\n\t\t{% if not no_letterhead and footer %}\r\n\t\t<div class=\"letter-head-footer\">\r\n\t\t\t{{ footer }}\r\n\t\t</div>\r\n\t\t{% endif %}\r\n\t</div>\r\n\r\n<div id=\"footer\">\r\n<img src=\"/files/Footer 1.PNG\" />\r\n</div>-->",
